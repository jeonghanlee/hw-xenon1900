# ReplyTimeout in the proto file should not be 0. 
# 
record(stringin, "$(SYSDEV)Vendor")
{
    field(DESC, "Vendor Name")
    field(DTYP, "stream")
    field( INP, "@xenon1900.proto read_vd $(PORT)")
    field(SCAN, "I/O Intr")
}

record(stringin, "$(SYSDEV)Formfactor")
{
    field(DESC, "Formfactor Type")
    field(DTYP, "Soft Channel")
}

record(stringin, "$(SYSDEV)Location")
{
    field(DESC, "Location defined by ICS internally")
    field(DTYP, "Soft Channel")
}

record(stringin, "$(SYSDEV)Status")
{
    field(DESC, "Status")
    field(DTYP, "Soft Channel")
}


record(stringin, "$(SYSDEV)Model")
{
    field(DESC, "Model Type")
    field(DTYP, "Soft Channel")
}


record(stringin, "$(SYSDEV)RawData")
{
    field(DESC, "RAW Scanned data")
    
    field(DTYP, "stream")
    field( INP, "@xenon1900.proto read_raw($(SYSDEV)SN) $(PORT)")
    field(SCAN, "I/O Intr")
}


record(stringin, "$(SYSDEV)SN")
{
    field(DESC, "Serial Number")
    field(DTYP, "Soft Channel")
 }


record(aSub,"$(SYSDEV)aSubExample")
{
    field(INAM,"myAsubInit")
    field(SNAM,"myAsubProcess")
    field(FTA,"STRING")
    field(NOA,"1")
    field(INPA,"$(SYSDEV)RawData CPP")
    field(FTVA,"STRING")
    field(OUTA, "$(SYSDEV)Formfactor" )
    field(FTVA, "STRING")
    field(OUTB, "$(SYSDEV)Model")
 }
