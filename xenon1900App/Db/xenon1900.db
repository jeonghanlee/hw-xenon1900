# ReplyTimeout in the proto file should not be 0. 
# 
record(stringin, "$(SYSDEV)RawScanData")
{
    field(DESC, "RAW Scanned data")
    field(DTYP, "stream")
    field( INP, "@xenon1900.proto read_raw $(PORT)")
    field(SCAN, "I/O Intr")
    field(PINI, "YES")
}
